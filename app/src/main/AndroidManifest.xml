<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="net.onlyid">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="唯ID"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:targetApi="m">
        <activity
            android:name=".user_info.EditActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".user_info.AvatarActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".app_manage.AppsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".device_manage.DevicesActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".scan_login.ScanActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".user_info.UserActivity"
            android:label="个人信息"
            android:screenOrientation="portrait" />
        <activity
            android:name=".LoginActivity"
            android:label="登录"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>